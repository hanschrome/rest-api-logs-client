  "type": "library",
  "description": "PHP library that adds a Client to connect with hanschrome/rest-api-logs.",
  "keywords": [
    "php",
    "logs",
    "http",
    "https",
    "client",
    "logger"
  ],
  "homepage": "https://github.com/hanschrome/rest-api-logs-client",
  "license": "MIT",
  "authors": [
    {"name": "Hans Castro", "email": "hanscastroadmn@gmail.com"}
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "RestApiLogsClient\\Common\\Client\\": "lib/RestApiLogsClient/Common/Client",
      "RestApiLogsClient\\Common\\Configuration\\": "lib/RestApiLogsClient/Common/Configuration",
      "RestApiLogsClient\\Common\\Exception\\": "lib/RestApiLogsClient/Common/Exception",
